{
  "name": "go-barber",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev:server": "ts-node-dev --inspect --transpile-only --ignore node_modules src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node-dev ./node_modules/typeorm/cli.js migration:generate",
    "migration:create": "ts-node-dev ./node_modules/typeorm/cli.js migration:create -n",
    "migration:run": "ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node-dev ./node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node-dev ./node_modules/typeorm/cli.js migration:revert",
    "migration:restart": "ts-node-dev ./node_modules/typeorm/cli.js schema:drop && ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "build": "tsc",
    "up": "docker start go-barber",
    "down": "docker stop go-barber"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.3.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "ts-node-dev": "^1.0.0-pre.60",
    "typescript": "^4.0.2"
  }
}
